<Window x:Class="Sasha_Project.ViewModels.DiologWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Settings" Height="300" Width="400">

    <Window.DataContext>
        <viewmodels:ViewModel/>
    </Window.DataContext>
    <Grid>
        <!-- Привяжите элементы интерфейса к свойствам ViewModel -->
        <TextBox Text="{Binding Settings.PropertyName}" />
        <!-- Добавьте кнопки сохранения и отмены -->
        <Button Content="Save" Command="{Binding SaveCommand}" />
        <Button Content="Cancel" Command="{Binding CancelCommand}" />
    </Grid>
</Window>
